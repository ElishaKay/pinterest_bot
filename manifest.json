{
  "manifest_version": 2,
  "name": "Pinterest Bot",
  "description": "pinterest bot",
  "version": "1.0",
  "content_scripts": [
    {
      "matches":["<all_urls>"],
      "js": [
        "js/app/content.js"
      ]
    }
  ],
  "background": {
    "scripts": ["js/app/background.js"]
  },
  "browser_action": {
    "default_icon": "assets/icons/curriculum_icon.png",
    "default_popup": "popup.html",
    "default_title": "Pinterest Bot"
  }
}
