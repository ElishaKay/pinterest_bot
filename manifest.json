{
  "manifest_version": 2,
  "name": "Pinterest Bot",
  "description": "pinterest bot",
  "version": "1.0",
  "content_scripts": [
    {
      "matches":["https://www.pinterest.com/*"],
      "js": [
        "js/app/content.js"
      ]
    }
  ],
  "browser_action": {
    "default_icon": "assets/icons/curriculum_icon.png",
    "default_popup": "views/popup.html",
    "default_title": "Pinterest Bot"
  }
}
